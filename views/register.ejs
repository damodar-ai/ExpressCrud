<body>
    <div class="maindiv">
        <h3 class="text-warning">Register Students From Here</h3>
    <form action = "/register" method="post">
        <div class="mb-3">
            <label for="exampleInputEmail1" class="form-label">Name Of Students</label>
            <input type="text" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" name = "name">
